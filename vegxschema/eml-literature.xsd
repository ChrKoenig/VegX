					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="totalPages" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<doc:tooltip>Total report pages</doc:tooltip>
						<doc:summary>The total number of pages in the report.</doc:summary>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PersonalCommunication">
